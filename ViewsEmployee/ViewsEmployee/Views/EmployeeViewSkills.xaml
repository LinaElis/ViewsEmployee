    <StackLayout Padding="5">
      <StackLayout>
        <Label Text="Skills" XAlign="Center" YAlign="Center" FontFamily="Arial" FontSize="17" TextColor="Fuchsia"></Label>

        <!--BindingContext="{Binding Skills}"-->
